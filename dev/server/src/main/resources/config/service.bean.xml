<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan base-package="coop.tecso.donde.estaciono.rest"/>
	<context:component-scan base-package="coop.tecso.donde.estaciono.config"/>

	<bean id="encryptService" class="coop.tecso.donde.estaciono.service.impl.EncryptServiceImpl" />
	
	<bean id="userService" class="coop.tecso.donde.estaciono.service.impl.UserServiceImpl" />
	
	<bean id="loginService" class="coop.tecso.donde.estaciono.service.impl.LoginServiceImpl" />
	
	<bean id="mobileRegisterService" class="coop.tecso.donde.estaciono.service.impl.MobileRegisterServiceImpl" />
	
	<bean id="securityService" class="coop.tecso.donde.estaciono.service.impl.SecurityServiceImpl" />
	
	<bean id="parkingService" class="coop.tecso.donde.estaciono.service.impl.ParkingServiceImpl" />
	
	<bean id="registerService" class="coop.tecso.donde.estaciono.service.impl.RegisterServiceImpl" />
	
	<bean id="hashGeneratorService" class="coop.tecso.donde.estaciono.service.impl.HashGeneratorServiceImpl" />

</beans>